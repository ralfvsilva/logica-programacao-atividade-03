programa {
  funcao inicio() {
    
    inteiro num = 0, count = 0, positivo = 0

    faca
    {

      escreva("Digite um numero: ")
      leia(num)

      se (num >= 0)
      {
        positivo += num
      }

    } enquanto (num != 0)

    escreva("A soma dos numeros positivos e: ", positivo)

  }
}
